generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model PrivacyPolicy {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  titulo      String
  descripcion String
  responsable String   // Nombre del responsable de la pol√≠tica
}